/* pages/statistics/statistics.wxss */

.checked-model {
    color: #fff;
    background-color: var(--haoyuebai--);
}

.checked-box {
    height: 100rpx;
    display: flex;
    justify-content: space-around;
    align-items: center;
}

.model-item {
    height: 60rpx;
    line-height: 60rpx;
    width: 150rpx;
    text-align: center;
    border-radius: 10rpx;
}

.active {
    background-color: var(--RIN-color--);
}